{
  "manifest_version": 3,
  "name": "WhatsApp transletter",
  "version": "1.0",
  "permissions": ["activeTab"],
  "action": {
    "default_popup": "index.html"
  },
  "icons": {
    "16": "icon16.png",
    "32": "icon32.png",
    "48": "icon48.png",
    "128": "icon128.png"

  },
  "web_accessible_resources": [
    {
      "resources": ["script.js", "countries.js", "style.css"],
      "matches": ["http://*/*", "https://*/*"]
    }
  ]
}